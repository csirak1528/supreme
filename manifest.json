{
  "manifest_version": 2,
  "name": "Supreme bot",
  "description": "Caleb Hermes Test",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Supreme bot"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}